}
a:hover, a:focus {
    text-decoration: none;
    color: #888888;
}
ul, ol {
    list-style: none;
    padding-inline-start: 0px;
}
.clearfix:after {
    visibility: hidden;
    display: block;
    font-size: 0;
    content: " ";
    clear: both;
    height: 0;
}
.clearfix {
    display: inline-block;
}
* html .clearfix {
    height: 1%;
}
.clearfix {
    display: block;
}
ul {
    list-style-type: none;
    padding: 0;
    margin: 0;
}

ul li {
